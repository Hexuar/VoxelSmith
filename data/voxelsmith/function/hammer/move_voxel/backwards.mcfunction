execute if score #dir voxelsmith.value matches 3 run scoreboard players remove @s voxelsmith.pos.x 1
execute if score #dir voxelsmith.value matches 0 run scoreboard players remove @s voxelsmith.pos.z 1
execute if score #dir voxelsmith.value matches 1 run scoreboard players add @s voxelsmith.pos.x 1
execute if score #dir voxelsmith.value matches 2 run scoreboard players add @s voxelsmith.pos.z 1